score = input(" Enter Score: ")
try:
    raw = float(score)
except: 
    raw = ('Out of range')
if raw >= 0.9 :
    print('A')
elif raw >= 0.8 :
    print('B')
elif raw >= 0.7 :
    print('C')
elif raw >= 0.6 :
    print('D')
elif raw < 0.6 :
    print('F')
else : 
    print('Out of range')
print ('All Done')